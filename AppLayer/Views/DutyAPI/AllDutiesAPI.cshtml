
<input id="sprintId" value="@ViewBag.SprintId"  hidden/>
<script>

    $(document).ready(function () {
        var param = $('#sprintId').val();

        $.ajax({
            url: '/DutyAPI/GetDutiesAPI?SprintId='+param,
            method: 'GET',
            dataType: 'json',
            success: function (items) {
                console.log(items);
            },
            error: function () {
                alert('error');
            }
        });

    });

</script>